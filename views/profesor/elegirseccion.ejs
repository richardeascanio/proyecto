<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="../assets/favicon.ico">

    <title>Template para busquedas especiales de estudiantes</title>

    <!-- Bootstrap core CSS -->
    <link href="../assets/styles/bootstrap.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="../assets/styles/template.css" rel="stylesheet">
  </head>

  <body>
      <form action="/profesor/BuscarEstudiantesdeSeccion/" class="form-signin2">
    <table class="table table-striped">
        <thead>
          <tr>
            <th>IDSeccion</th>
        
          </tr>
        </thead>
        <tbody>
          <tr>
            <% _.each(secciones, function(seccion) {%>
              <th scope="row">
                <%=seccion.idseccion%>
              </th>    
          </tr>
          <%}) %>
        </tbody>
      </table> 

      <label for="idseccion" class="sr-only">ID de la Sección a escoger</label>
      <input type="int" name="idseccion" class="form-control" placeholder="ID de la Sección" required>
      <hr class="mb-4">
      <button class="btn btn-lg btn-primary btn-block" type="submit">Siguiente</button>
      <p class="mt-5 mb-3 text">&copy; Los que flinchy</p>
      </form> 

      </body>
  
</html>